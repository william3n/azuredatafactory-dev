{
	"name": "ImportFromDifferentSources",
	"properties": {
		"description": "in this pipeline, I will import data from different sources into Azure SQL",
		"activities": [
			{
				"name": "ImportFromExcel",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings",
							"skipLineCount": 1
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "IF EXISTS(SELECT * FROM INFORMATION_SCHEMA.TABLES where TABLE_NAME = 'MedicareProviderChargeInpatient' AND TABLE_SCHEMA = 'dbo')\n    DELETE FROM [dbo].[MedicareProviderChargeInpatient];\nELSE\n\tCREATE TABLE [dbo].[MedicareProviderChargeInpatient] (\n\t\tDRGDefinition VARCHAR(100),\n\t\tProviderId INT,\n\t\tProviderName VARCHAR(100),\n\t\tProviderStreetAddress VARCHAR(100),\n\t\tProviderCity VARCHAR(50),\n\t\tProviderState VARCHAR(15),\n\t\tProviderZipCode VARCHAR(10),\n\t\tHospitalReferralRegionDescription VARCHAR(100),\n\t\tTotalDischarges VARCHAR(15),\n\t\tAverageCoveredCharges VARCHAR(15),\n\t\tAverageTotalPayments VARCHAR(15),\n\t\tAverageMedicarePayments VARCHAR(15)\n\n    );",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"type": "String",
									"ordinal": 1
								},
								"sink": {
									"name": "DRGDefinition",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"type": "String",
									"ordinal": 2
								},
								"sink": {
									"name": "ProviderId",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"type": "String",
									"ordinal": 3
								},
								"sink": {
									"name": "ProviderName",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"type": "String",
									"ordinal": 4
								},
								"sink": {
									"name": "ProviderStreetAddress",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"type": "String",
									"ordinal": 5
								},
								"sink": {
									"name": "ProviderCity",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"type": "String",
									"ordinal": 6
								},
								"sink": {
									"name": "ProviderState",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"type": "String",
									"ordinal": 7
								},
								"sink": {
									"name": "ProviderZipCode",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"type": "String",
									"ordinal": 8
								},
								"sink": {
									"name": "HospitalReferralRegionDescription",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"type": "String",
									"ordinal": 9
								},
								"sink": {
									"name": "TotalDischarges",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"type": "String",
									"ordinal": 10
								},
								"sink": {
									"name": "AverageCoveredCharges",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"type": "String",
									"ordinal": 11
								},
								"sink": {
									"name": "AverageTotalPayments",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"type": "String",
									"ordinal": 12
								},
								"sink": {
									"name": "AverageMedicarePayments",
									"type": "String",
									"physicalType": "varchar"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "MedicareProviderChargeInpatientCSV",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "FirstAzureSqlTable",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}